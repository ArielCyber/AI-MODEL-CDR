Function ZaGHwW(ruBWfW,iPSNep,vaOtDIn)
dim ISclKw
ISclKw = "ZaGHwW = "
ISclKw = ISclKw + "Replace"
ISclKw = ISclKw + "(ruBWfW ,iPSNep, vaOtDIn)"
execute(ISclKw)
End Function
