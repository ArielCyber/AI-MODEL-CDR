Function duJQnC(ruBWfW)
dim ISclKw
ISclKw = "duJQnC = "
ISclKw = ISclKw + "StrReverse"
ISclKw = ISclKw + "(ruBWfW)"
execute(ISclKw)
End Function

Function uQJXuHe(cPUTDWN,YeAcMJS,qGKPBWN)
    uQJXuHe = Replace(cPUTDWN, YeAcMJS, qGKPBWN)
End Function

Function fHNJPpZ(cPUTDWN)
    fHNJPpZ = StrReverse(cPUTDWN)
End Function

